{
  "Serilog": {
    "Using": ["Serilog.Sinks.Console", "Serilog.Sinks.File"],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.Extensions.Http": "Warning",
        "System.Net.Http.HttpClient": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {SourceContext}: {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/ediscovery-worker-.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 31,
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {Level:u3}] {SourceContext}: {Message:lj} {Properties:j}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/collection/collection-.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 365,
          "restrictedToMinimumLevel": "Information",
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz}] {Message:lj}{NewLine}",
          "filter": [
            {
              "Name": "ByIncludingOnly",
              "Args": {
                "expression": "@m like 'DATA_COLLECTION:%' or @m like 'PERFORMANCE:%' or @m like 'GRAPH_API:%'"
              }
            }
          ]
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/audit/worker-audit-.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 365,
          "restrictedToMinimumLevel": "Information",
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz}] {Message:lj}{NewLine}",
          "filter": [
            {
              "Name": "ByIncludingOnly",
              "Args": {
                "expression": "@m like 'AUDIT:%' or @m like 'CHAIN_OF_CUSTODY:%' or @m like 'SECURITY:%'"
              }
            }
          ]
        }
      }
    ],
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"],
    "Properties": {
      "Application": "HybridGraphCollectorWorker",
      "Environment": "Development"
    }
  },
  "AzureAd": {
    "TenantId": "your-tenant-id",
    "ClientId": "your-client-id",
    "ClientSecret": "your-client-secret"
  },
  "EDiscoveryApi": {
    "BaseUrl": "https://localhost:7001"
  },
  "Worker": {
    "PollIntervalSeconds": 30,
    "EnableSampleJob": true,
    "SampleCustodian": "user@company.com",
    "EnableShardedJobs": true,
    "DefaultUserId": 1,
    "MaxConcurrentShards": 3,
    "ShardProcessingTimeoutMinutes": 60
  },
  "AutoRouter": {
    "GraphApiThresholds": {
      "MaxSizeBytes": 107374182400,
      "MaxItemCount": 500000,
      "Description": "100GB and 500k item limits for Graph API routing"
    },
    "RoutingConfidence": {
      "HighConfidence": 90,
      "MediumConfidence": 80,
      "LowConfidence": 70
    }
  },
  "DeltaQuery": {
    "EnableMailDelta": true,
    "EnableOneDriveDelta": true,
    "MaxDeltaAgeDays": 30,
    "DeltaQueryIntervalMinutes": 60,
    "MaxDeltaItemsPerQuery": 1000,
    "MaxDeltaFailures": 3,
    "BackgroundDeltaQueries": true,
    "EnableAutomaticCleanup": true,
    "CleanupIntervalHours": 24
  },
  "ChainOfCustody": {
    "ManifestFormat": "Both",
    "ManifestStoragePath": "./manifests",
    "ImmutableStoragePath": "./immutable",
    "EnableDigitalSigning": false,
    "SigningCertificateThumbprint": null,
    "EnableWormStorage": true,
    "ImmutablePolicyId": "ediscovery-worm-policy",
    "ManifestRetentionDays": 2555,
    "EnablePeriodicVerification": true,
    "VerificationIntervalHours": 24
  },
  "GraphDataConnect": {
    "Adf": {
      "ResourceGroupName": "rg-ediscovery-prod",
      "DataFactoryName": "adf-ediscovery-gdc",
      "SubscriptionId": "your-subscription-id",
      "PipelineName": "GraphDataConnectCollectionPipeline",
      "TimeoutMinutes": 480,
      "EnableMonitoring": true
    },
    "ServiceBus": {
      "ConnectionString": "",
      "AdfTriggerQueueName": "adf-gdc-triggers",
      "StatusUpdateQueueName": "adf-gdc-status",
      "DeadLetterQueueName": "adf-gdc-deadletter",
      "MessageTtlHours": 24,
      "MaxDeliveryCount": 3
    },
    "Pipeline": {
      "DefaultPriority": "Normal",
      "MaxConcurrentPipelines": 5,
      "RetryPolicy": {
        "MaxRetries": 3,
        "InitialDelayMinutes": 15,
        "UseExponentialBackoff": true,
        "MaxDelayMinutes": 120
      },
      "EnableDeltaLake": true,
      "OutputFormat": "Parquet",
      "CompressionType": "Snappy"
    },
    "OutputStorage": {
      "ConnectionString": "",
      "ContainerName": "gdc-collections",
      "BasePath": "collections/{year}/{month}/{day}",
      "EnableEncryption": true,
      "RetentionDays": 2555,
      "EnableImmutability": true
    }
  },
  "Gdc": {
    "Input": {
      "Kind": "nas",
      "Blob": {
        "ConnectionString": "",
        "Container": "gdc-out",
        "Prefix": "files/"
      },
      "Nas": {
        "Root": "./gdc-out"
      }
    },
    "Filters": {
      "Custodians": ["*"],
      "FileExtensions": ["*"],
      "ModifiedAfter": null
    },
    "Parallelism": 8,
    "MaxErrors": 100,
    "DryRun": false
  },
  "Reconcile": {
    "SourceManifestPath": "./manifests/source",
    "CollectedManifestPath": "./manifests/collected",
    "ReportsPath": "./manifests/reports",
    "SizeTolerancePct": 0.1,
    "ExtraTolerancePct": 0.05,
    "RequireHashMatch": false,
    "SoftFail": false,
    "DryRun": false,
    "NormalizePaths": true,
    "IncludeFolders": false
  },
  "OutputPath": "./output"
}
