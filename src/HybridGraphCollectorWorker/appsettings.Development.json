{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.Extensions.Http": "Information",
        "System.Net.Http.HttpClient": "Information"
      }
    }
  },
  "AzureAd": {
    "TenantId": "development-tenant-id",
    "ClientId": "development-client-id",
    "ClientSecret": "development-client-secret"
  },
  "EDiscoveryApi": {
    "BaseUrl": "https://localhost:5230"
  },
  "Worker": {
    "PollIntervalSeconds": 15,
    "EnableSampleJob": true,
    "SampleCustodian": "test.user@development.com"
  },
  "AutoRouter": {
    "GraphApiThresholds": {
      "MaxSizeBytes": 10737418240,
      "MaxItemCount": 50000,
      "Description": "10GB and 50k item limits for development/testing Graph API routing"
    },
    "RoutingConfidence": {
      "HighConfidence": 95,
      "MediumConfidence": 85,
      "LowConfidence": 75
    }
  },
  "DeltaQuery": {
    "EnableMailDelta": true,
    "EnableOneDriveDelta": true,
    "MaxDeltaAgeDays": 7,
    "DeltaQueryIntervalMinutes": 15,
    "MaxDeltaItemsPerQuery": 100,
    "MaxDeltaFailures": 2,
    "BackgroundDeltaQueries": true,
    "EnableAutomaticCleanup": true,
    "CleanupIntervalHours": 6
  },
  "OutputPath": "./output"
}
